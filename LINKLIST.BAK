

#include<stdio.h>
#include<conio.h>
struct node
{
int data;
struct node *next;
}*head=NULL,*temp=NULL;

int addAtLastPos(int data)
{
struct node*start=head;
temp=(struct node*)malloc(sizeof(struct node));
temp->data=data;
temp->next=NULL;
while(start->next!=NULL)
start=start->next;
start->next=temp;
return 1;
}
void printdata()
{
 struct node*start=head;
 while(start!=NULL)
 {
 printf("%d->",start->data);
 start=start->next;
 }
 printf("NULL");
}

